	rdfs:subClassOf
		fibo-fnd-arr-cls:ClassificationScheme ,
		lcc-lr:CodeSet ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:hasMember ;
			owl:allValuesFrom fibo-cae-ce-srca:CorporateActionClassifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:defines ;
			owl:someValuesFrom fibo-cae-ce-srca:CorporateActionClassifier ;
		]
		;
	rdfs:label "corporate action classification scheme" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/CAE/CorporateEvents/SecurityRelatedCorporateActions/> ;
	skos:definition "scheme for classifying corporate actions" ;
	fibo-fnd-utl-av:usageNote "The set of corporate actions and income events included herein are a subset of those specified in ISO 15022 Securities - Scheme for Messages (Data Field Dictionary). Other schemes that are specific to a custodian, depository, or regulatory agency may also be important, and should take a similar approach with respect to classification." ;
	.

fibo-cae-ce-srca:CorporateActionClassifier
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-cls:Classifier ,
		lcc-lr:CodeElement ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isDefinedIn ;
			owl:onClass fibo-cae-ce-srca:CorporateActionClassificationScheme ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:isMemberOf ;
			owl:onClass fibo-cae-ce-srca:CorporateActionClassificationScheme ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-cr:classifies ;
			owl:someValuesFrom fibo-cae-ce-srca:CorporateAction ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:denotes ;
			owl:someValuesFrom fibo-cae-ce-srca:CorporateAction ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:hasTag ;
			owl:someValuesFrom xsd:string ;
		]
		;
	rdfs:label "corporate action classifier"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/CAE/CorporateEvents/SecurityRelatedCorporateActions/> ;
	skos:definition "classifier that distinguishes income-oriented events from other corporate actions"@en ;
	fibo-fnd-utl-av:explanatoryNote "ISO 15022 classifies events as impacting income vs. others.  Other classification schemes distinguish between actions that return profits to shareholders, actions that are designed to influence the share price, and actions involving a change in structure to the issuer organization."@en ;
	.

